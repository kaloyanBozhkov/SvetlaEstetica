{
  "name": "svetlaestetica",
  "version": "1.0.0",
  "description": "a MERN stack based ecommerce website",
  "main": "index.js",
  "scripts": {
    "devServer": "nodemon server/server.js NODE_ENV=development --watch",
    "lint": "eslint ./src/** -c ./eslintrc.json --ext js,jsx",
    "dev": "next",
    "start": "next start",
    "build": "next build",
    "test": "jest --watchAll",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaloyanBozhkov/SvetlaEstetica.git"
  },
  "keywords": [
    "MongoDB",
    "Express",
    "React",
    "Node",
    "Next"
  ],
  "author": "Kaloyan Bozhkov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kaloyanBozhkov/SvetlaEstetica/issues"
  },
  "homepage": "https://github.com/kaloyanBozhkov/SvetlaEstetica#readme",
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@zeit/next-sass": "^1.0.1",
    "emotion-theming": "^10.0.27",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "framer-motion": "^2.9.4",
    "graphql": "^15.4.0",
    "isomorphic-fetch": "^3.0.0",
    "next-images": "^1.6.2",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^6.0.2",
    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-datepicker": "^2.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.1.1",
    "react-relay": "^10.1.0",
    "react-router": "^5.1.2",
    "react-scripts": "^4.0.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
